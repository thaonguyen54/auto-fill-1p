{
  "name": "auto-fill-1p",
  "version": "1.0.0",
  "main": "./build/main.bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:main": "webpack --config webpack.config.main.js && electron .",
    "build:renderer": "webpack-dev-server --hot --config webpack.config.renderer.js",
    "watch:static": "webpack --watch --config webpack.config.renderer.js",
    "start": "concurrently -k \"npm:watch:static\" \"npm:build:renderer\" \"npm:build:main\"",
    "remove-previous-config": "cd configs && del config.js && cd ..",
    "config": "set ENV=dev&& npm run remove-previous-config && node ./src/scripts/script.js",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "appId": "com.auto-fill-1p",
    "productName": "Auto Fill 1P",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "./build/main.js",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.461.0",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
